programa {
	funcao inicio() {
	    inteiro tamanho
		
		escreva ("Qual o tamanho? ")
		leia(tamanho)
		
		se (tamanho > 14) {
		    escreva ("Vai catar manga?")
		}
		
		se (tamanho < 10) {
		    escreva ("Minhoquinha.")
		}
		
	    se (tamanho < 15 e tamanho > 9) {
	        escreva ("Tá na média.")
	    }
	    
	}
}
